{
  "$schema": "node_modules/turbo/schema.json",
  "dangerouslyDisablePackageManagerCheck": true,
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "biome-check": {
      "cache": false
    },
    "biome-fix": {
      "cache": false
    },
    "build": {
      "cache": true,
      "outputs": ["build"],
      "dependsOn": ["^build"]
    },
    "dockerize": {
      "cache": false,
      "dependsOn": ["build", "^dockerize"]
    }
  }
}
